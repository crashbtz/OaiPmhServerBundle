{% extends 'NaonedOaiPmhServerBundle::template.xml.twig' %}

{% block content %}
	<Identify>
		<repositoryName>{{ dataProvider.repositoryName }}</repositoryName>
		<baseURL>{{ url(app.request.attributes.get('_route')) }}</baseURL>
		<protocolVersion>2.0</protocolVersion>
		<earliestDatestamp>{{ dataProvider.earliestDatestamp|date('Y-m-d\\T') }}</earliestDatestamp>
		<deletedRecord>no</deletedRecord>
		<granularity>YYYY-MM-DDT</granularity>
		<adminEmail>{{ dataProvider.adminEmail }}</adminEmail>
	</Identify>
{% endblock %}