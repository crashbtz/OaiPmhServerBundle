{% extends 'NaonedOaiPmhServerBundle::template.xml.twig' %}

{% block content %}
  <ListRecords>
    {% for position in starts..ends %}
      {% include 'NaonedOaiPmhServerBundle::record.xml.twig' with {record: records[position]} %}
    {% endfor %}
    {% include 'NaonedOaiPmhServerBundle::resumptionToken.xml.twig' with {totalCount: records|length} %}
  </ListRecords>
{% endblock %}