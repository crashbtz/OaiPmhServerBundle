
<resumptionToken
  {% if resumption %}expirationDate="{{ resumption.expiresOn|date('Y-m-d\\TH:i:sP') }}"{% endif %}
  completeListSize="{{ paginationData.totalCount }}"
  cursor="{{ (paginationData.current-1)*paginationData.numItemsPerPage }}"
>{% if resumption %}{{ resumption.token }}{% endif %}</resumptionToken>
